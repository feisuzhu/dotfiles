[user]
	name = Proton
	email = feisuzhu@163.com
[branch]
    autosetuprebase = always
[merge]
	conflictstyle = diff3
[color]
    ;status = auto
	diff = auto
	branch = auto
	interactive = auto
	grep = auto
[core]
    editor = vim
	pager = diff-so-fancy | less --tabs=4 -RFX
	fsmonitor = true
	untrackedCache = true
[alias]
  abbr = "!sh -c 'git rev-list --all | grep ^$1 | while read commit; do git --no-pager log -n1 --pretty=format:5784acef31ec68226b79a9858161558597a1aac9quot;%H %ci %an %s%n5784acef31ec68226b79a9858161558597a1aac9quot; $commit; done' -"
  aliases = !git config --get-regexp 'alias.*' | colrm 1 6 | sed 's/[ ]/ = /'
  ci = commit
  co = checkout
  st = status -sb
  lc = log ORIG_HEAD.. --stat --no-merges
  lg = log --graph --pretty=oneline --abbrev-commit
  lgg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  diffstat = diff-tree --stat
  quickserve = daemon --verbose --export-all --base-path=.git --reuseaddr --strict-paths .git/
[push]
	default = matching
[safe]
	directory = /var/lib/git-cache
	directory = /home/proton/actions-runner/_work/taichi/taichi
	directory = /home/proton/actions-runner/feisuzhu/_work/Taichi-UnityExample/Taichi-UnityExample
	directory = /home/proton/git-cache
	directory = /home/proton/actions-runner/taichi/_work/taichi/taichi
[rerere]
	enabled = true
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
