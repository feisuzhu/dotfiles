git clone https://github.com/feisuzhu/dotfiles.git
cd dotfiles
DIR=`pwd`
cd ~
mkdir -p .vim .config
ln -svf $DIR/.ackrc ~/
ln -svf $DIR/.agignore ~/
ln -svf $DIR/.ctags ~/
ln -svf $DIR/.gitconfig ~/
ln -svf $DIR/.vimrc ~/
ln -svf $DIR/.gvimrc ~/
ln -svf $DIR/.zshrc ~/
ln -svf $DIR/flake8 ~/.config/
ln -svf $DIR/vimscripts .vim/vimscripts
vim -c q
